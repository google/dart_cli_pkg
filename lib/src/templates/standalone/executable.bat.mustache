@echo off
REM This script drives the standalone {{{name}}} package, which bundles together a
REM Dart executable and a snapshot of {{{name}}}.

set SCRIPTPATH=%~dp0
set arguments=%*
"%SCRIPTPATH%\src\dart.exe" {{#version}}"-Dversion={{{version}}}"{{/version}} "%SCRIPTPATH%\src\{{{executable}}}.snapshot" %arguments%
